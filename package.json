{
    "name": "server",
    "version": "1.0.0",
    "description": "Backend of my blog",
    "main": "app.js",
    "scripts": {
        "start": "node dist/app",
        "dev": "nodemon src/app.ts",
        "build": "gulp",
        "test": "jest",
        "lint:es": "npx eslint .",
        "lint:es:fix": "npx eslint . --fix",
        "prettier": "npx prettier -c .",
        "style": "npm run lint:es && npm run prettier",
        "prepare": "husky install"
    },
    "author": "Cloudless67",
    "license": "MIT",
    "devDependencies": {
        "@tsconfig/node16": "^1.0.2",
        "@types/babel__core": "^7.1.16",
        "@types/bcrypt": "^3.0.1",
        "@types/connect-history-api-fallback": "^1.3.4",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.22",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/katex": "^0.11.0",
        "@types/luxon": "^1.26.2",
        "@types/marked": "^2.0.1",
        "@types/mongoose": "^5.10.4",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.37",
        "@types/prismjs": "^1.16.5",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.33.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.24.2",
        "husky": "^7.0.0",
        "jest": "^26.6.3",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "connect-history-api-fallback": "^1.6.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-static-gzip": "^2.1.1",
        "helmet": "^4.4.1",
        "jsonwebtoken": "^8.5.1",
        "katex": "^0.13.3",
        "luxon": "^1.26.0",
        "marked": "^2.0.1",
        "mongoose": "^5.12.2",
        "morgan": "^1.10.0",
        "prismjs": "^1.23.0"
    },
    "nodemonConfig": {
        "restartable": "rs",
        "ignore": [
            ".git",
            "node_modules/",
            "dist/",
            "coverage/"
        ],
        "watch": [
            "src/"
        ],
        "execMap": {
            "ts": "node -r ts-node/register"
        },
        "env": {
            "NODE_ENV": "development"
        },
        "ext": "js,json,ts"
    }
}
